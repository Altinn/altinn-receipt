{
  "name": "receipt-react-app",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageReporters=lcov",
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint \"./src/**/*.ts*\"",
    "compile-ts": "tsc",
    "clean": "rimraf dist compiled"
  },
  "author": "Altinn",
  "license": "3-Clause BSD",
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "@material-ui/core": "4.12.4",
    "axios": "1.12.2",
    "classnames": "2.5.1",
    "dompurify": "3.2.7",
    "html-react-parser": "5.2.6",
    "marked": "15.0.12",
    "moment": "2.30.1",
    "react": "18.3.1",
    "react-content-loader": "7.1.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-react": "7.27.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/dompurify": "3.2.0",
    "@types/jest": "29.5.14",
    "@types/jsdom": "21.1.7",
    "@types/marked": "6.0.0",
    "@types/react": "18.3.25",
    "@types/react-dom": "18.3.7",
    "@typescript-eslint/eslint-plugin": "8.45.0",
    "@typescript-eslint/parser": "8.45.0",
    "altinn-designsystem": "4.2.0",
    "babel-jest": "29.7.0",
    "babel-loader": "10.0.0",
    "cross-env": "7.0.3",
    "css-loader": "7.1.2",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "fork-ts-checker-notifier-webpack-plugin": "9.0.0",
    "fork-ts-checker-webpack-plugin": "9.1.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-fixed-jsdom": "^0.0.10",
    "jest-junit": "16.0.0",
    "jsdom": "26.1.0",
    "mini-css-extract-plugin": "2.9.4",
    "msw": "2.11.3",
    "prettier": "3.6.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rimraf": "6.0.1",
    "source-map-loader": "5.0.0",
    "terser-webpack-plugin": "5.3.14",
    "ts-jest": "29.4.4",
    "ts-loader": "9.5.4",
    "typescript": "4.9.5",
    "undici": "^7.3.0",
    "webpack": "5.102.0",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2"
  },
  "packageManager": "yarn@3.8.7",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^testConfig/(.*)$": "<rootDir>/testConfig/$1"
    },
    "testRegex": "(/__tests__/.*|.*.(test|spec)).(ts|tsx|js|jsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/testConfig/setupTests.ts"
    ],
    "testEnvironmentOptions": {
      "url": "https://localhost/receipt/mockInstanceOwnerId/6697de17-18c7-4fb9-a428-d6a414a797ae",
      "customExportConditions": [
        ""
      ]
    },
    "testEnvironment": "jest-fixed-jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "collectCoverageFrom": [
    "!__tests__/**/*",
    "src/**/*.{ts,tsx}"
  ]
}
