{
  "name": "receipt-react-app",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.development.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "test": "jest",
    "lint": "eslint \"./src/**/*.ts*\"",
    "compile-ts": "tsc",
    "clean": "rimraf dist compiled"
  },
  "author": "Altinn",
  "license": "3-Clause BSD",
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "@material-ui/core": "4.12.4",
    "axios": "0.27.2",
    "classnames": "2.3.2",
    "dompurify": "3.0.5",
    "html-react-parser": "4.0.0",
    "marked": "5.1.1",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-content-loader": "6.2.1",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@babel/preset-env": "7.22.9",
    "@babel/preset-react": "7.22.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/dompurify": "3.0.2",
    "@types/jest": "29.5.3",
    "@types/jsdom": "21.1.1",
    "@types/marked": "5.0.1",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "altinn-designsystem": "4.2.0",
    "babel-jest": "29.6.1",
    "babel-loader": "9.1.3",
    "cross-env": "7.0.3",
    "css-loader": "6.8.1",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fork-ts-checker-notifier-webpack-plugin": "7.0.0",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "jest": "29.6.1",
    "jest-environment-jsdom": "29.6.1",
    "jest-junit": "16.0.0",
    "jsdom": "22.1.0",
    "mini-css-extract-plugin": "2.7.6",
    "msw": "1.2.2",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.1",
    "source-map-loader": "4.0.1",
    "terser-webpack-plugin": "5.3.9",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "typescript": "4.9.5",
    "webpack": "5.88.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "packageManager": "yarn@3.6.1",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^testConfig/(.*)$": "<rootDir>/testConfig/$1"
    },
    "testRegex": "(/__tests__/.*|.*.(test|spec)).(ts|tsx|js|jsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/testConfig/setupTests.ts"
    ],
    "testEnvironmentOptions": {
      "url": "https://localhost/receipt/mockInstanceOwnerId/6697de17-18c7-4fb9-a428-d6a414a797ae"
    },
    "testEnvironment": "jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "collectCoverageFrom": [
    "!__tests__/**/*",
    "src/**/*.{ts,tsx}"
  ]
}
